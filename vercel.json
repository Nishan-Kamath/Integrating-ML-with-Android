{
  "version": 2,

  "functions": {
    "*.py": {
      "runtime": "python3.11"
    }
  },

  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
